! $HOME/.Xdefaults
URxvt.preeditType:Root
URxvt.inputMethod:fcitx

! 颜色设置
URxvt.depth:32
! 透明度
URxvt.transparent:True
URxvt.shading:35
! 背景色
URxvt.background:#000000
URxvt.foreground:#FFFFFF
URxvt.colorBD:#F2F2F2
URxvt.colorUL:#00FF00

URxvt.color1:#EE0000
URxvt.color4:#4169E1
URxvt.color5:#EE00EE
URxvt.color8:#7f7f7f
URxvt.color10:#00EE00
URxvt.color12:#1E90FF
URxvt.color14:#00EEEE
URxvt.color15:#F2F2F2

