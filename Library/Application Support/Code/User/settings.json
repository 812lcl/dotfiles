{
  "workbench.colorTheme": "Monokai",
  "workbench.preferredDarkColorTheme": "Monokai",
  "editor.minimap.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 2,
  "editor.wordWrap": "on",
  "editor.fontSize": 14,
  "editor.codeLensFontFamily": "Source Code Pro for Powerline",
  "editor.fontFamily": "Source Code Pro for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "debug.console.fontFamily": "Source Code Pro for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "chat.editor.fontFamily": "Source Code Pro for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "window.zoomLevel": 1,
  "explorer.excludeGitIgnore": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.coverOnSingleTest": true,
  "go.coverOnSingleTestFile": true,
  "go.formatTool": "goimports",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.ignorecase": true,
  "vim.smartcase": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.easymotion": true,
  "vim.leader": "\\",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["g", "y"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["g", "h"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["h", "g"],
      "commands": ["workbench.action.editor.previousChange"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["f"],
      "after": ["leader", "leader", "s"]
    },
    {
      "before": ["<c-h>"],
      "after": ["<c-w>", "h"]
    },
    {
      "before": ["<c-j>"],
      "after": ["<c-w>", "j"]
    },
    {
      "before": ["<c-k>"],
      "after": ["<c-w>", "k"]
    },
    {
      "before": ["<c-l>"],
      "after": ["<c-w>", "l"]
    },
    {
      "before": ["s", "g"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["s", "v"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["leader", "1"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": ["leader", "2"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": ["leader", "3"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": ["leader", "4"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    {
      "before": ["leader", "5"],
      "commands": ["workbench.action.openEditorAtIndex5"]
    },
    {
      "before": ["leader", "6"],
      "commands": ["workbench.action.openEditorAtIndex6"]
    },
    {
      "before": ["leader", "7"],
      "commands": ["workbench.action.openEditorAtIndex7"]
    },
    {
      "before": ["leader", "8"],
      "commands": ["workbench.action.openEditorAtIndex8"]
    },
    {
      "before": ["leader", "9"],
      "commands": ["workbench.action.openEditorAtIndex9"]
    }
  ],
  "terminal.integrated.fontWeight": "normal",
  "git.openRepositoryInParentFolders": "always",
  "gitlens.views.searchAndCompare.files.layout": "tree",
  "gitlens.hovers.currentLine.over": "line",
  "terminal.integrated.defaultProfile.osx": "tmux",
  "gopls": {
    "ui.semanticTokens": true
  },
  "git.confirmSync": false,
  "editor.semanticTokenColorCustomizations": {}
}