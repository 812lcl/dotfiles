{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "agents": {
    "sisyphus": {
      "model": "sk-router/gpt-5.2-pro"
    },
    "hephaestus": {
      "model": "sk-router/gpt-5.2-pro"
    },
    "prometheus": {
      "model": "sk-router/gpt-5.2-pro"
    },
    "librarian": {
      "model": "sk-router/claude-sonnet-4.5"
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "oracle": {
      "model": "sk-router/gpt-5.2-pro"
    },
    "frontend-ui-ux-engineer": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "document-writer": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "metis": {
      "model": "sk-router/claude-sonnet-4.5"
    }
  },
  "background_task": {
    "defaultConcurrency": 5,
    "providerConcurrency": {
      "anthropic": 3,
      "openai": 5,
      "google": 10,
      "sk-sky": 5,
      "sk-xmind": 5,
      "sk-router": 5
    },
    "modelConcurrency": {
      "anthropic/claude-opus-4-5": 2,
      "google/gemini-3-flash": 10,
      "sk-router/gpt-5.2-pro": 2
    }
  }
}
